{
    "display_information": {
        "name": "mem-cho (cmueats uptime monitor)"
    },
    "features": {
        "bot_user": {
            "display_name": "mem-cho (cmueats uptime monitor)",
            "always_online": true
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "chat:write.public",
                "chat:write",
                "channels:read"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_home_opened",
                "message.channels"
            ]
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}